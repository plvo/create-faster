{
  "name": "{{appName}}",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    {{#if (eq repo "single")}}
    {{#if (eq linter "biome")}}
    "format": "biome format --write .",
    "lint": "biome lint",
    {{/if}}
    {{#if (eq linter "eslint")}}
    "format": "eslint --fix .",
    "lint": "eslint",
    {{/if}}
    {{/if}}
    "start": "expo start -c",
    "start:android": "expo start --android -c",
    "start:ios": "expo start --ios -c",
    "start:web": "expo start --web -c",
    "prebuild:android": "expo prebuild --platform android --clean",
    "prebuild:ios": "expo prebuild --platform ios --clean",
    "build:eas:android": "eas build --platform android --profile production",
    "build:eas:ios": "eas build --platform ios --profile production",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.20",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.9",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.11",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.8",
    {{#if (hasModule "nativewind")}}
    "nativewind": "^4.1.23",
    {{/if}}
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-worklets": "0.5.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0"
  },
  "devDependencies": {
    {{#if (eq repo "turborepo")}}
    {{#if (eq linter "eslint")}}
    "@repo/eslint-config": "*",
    {{/if}}
    {{/if}}
    "@babel/core": "^7.25.2",
    "@types/react": "^19",
    {{#if (hasModule "nativewind")}}
    "tailwindcss": "3.3.2",
    {{/if}}
    {{#if (eq repo "single")}}
    {{#if (eq linter "biome")}}
    "@biomejs/biome": "^2.3.11",
    {{/if}}
    {{#if (eq linter "eslint")}}
    "eslint": "^9",
    "@eslint/js": "^9",
    "typescript-eslint": "^8",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^5",
    "eslint-plugin-react-native": "^4",
    {{/if}}
    {{#if (hasExtra "husky")}}
    "husky": "^9.1.7",
    {{/if}}
    {{/if}}
    "typescript": "^5"
  },
  "private": true
}
